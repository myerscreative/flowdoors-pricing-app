{
  "name": "flowdoors-pricing-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "postinstall": "pnpm prisma:generate && simple-git-hooks",
    "dev": "pnpm dev:ports && next dev -p 3000",
    "dev:ports": "lsof -ti TCP:3000,3001,3002 2>/dev/null | xargs kill -9 2>/dev/null || true",
    "clean": "rm -rf .next .turbo .vercel/out node_modules/.cache",
    "dev:reset": "pnpm dev:ports && pnpm clean && pnpm install --force && next dev -p 3000",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 next build",
    "start": "next start -p 3000",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init_notes",
    "prisma:studio": "prisma studio",
    "lint": "next lint",
    "prepare": "husky",
    "format": "prettier --write .",
    "set-role": "tsx scripts/setUserRole.ts",
    "list-users": "tsx scripts/listUsersWithRoles.ts"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm typecheck"
  },
